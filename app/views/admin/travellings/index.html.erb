<div class="col-lg-12 grid-margin stretch-card">
  <div class="card">
    <div class="card-body">
      <div>
        <%= link_to t(".click"), new_admin_travelling_path, class: "btn btn-primary btn-lg" %>
      </div>
      <br>
      <div class="table-responsive">
        <table class="table table-hover">
          <thead>
            <tr>
              <th><%= t ".id" %></th>
              <th><%= t ".start" %></th>
              <th><%= t ".end" %></th>
              <th></th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <%= render @travellings %>
          </tbody>
        </table>
        <%= will_paginate %>
      </div>
    </div>
  </div>
</div>
